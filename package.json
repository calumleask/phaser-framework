{
  "name": "phaser-framework",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/phfw.js",
  "module": "./src/index.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "test": "run-s test:lint test:unit",
    "test:lint": "eslint --ext .ts src -c .eslintrc",
    "test:unit": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack --config webpack.dev.js --progress",
    "watch": "webpack --config webpack.dev.js --progress --watch",
    "start": "run-s watch",
    "build": "webpack --config webpack.prod.js --progress",
    "ts:defs": "tsc --declaration --outDir ./dist/types --emitDeclarationOnly",
    "format": "prettier --write src"
  },
  "author": "Calum Leask",
  "license": "ISC",
  "dependencies": {
    "phaser": "^3.55.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^16.18.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.8.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "types": "./types/phfw.d.ts"
